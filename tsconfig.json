{
    "compilerOptions": {
      "outDir": "dist",
      "module": "commonjs",
      "target": "es2016",
      "lib": ["ES2020"],
      "sourceMap": true,
      "strict": true,
      "moduleResolution": "node",
      "declaration": true,
      "esModuleInterop": true,
      "jsx": "react",
      "typeRoots": [
        "node_modules/@types",
        "./@types"
      ]
    },
    "include": [
      "srcold/**/*.ts", "srcold/commands/redeem2.tsx"
    ],
    "exclude": [
      "**/*.spec.ts"
    ]
  }
